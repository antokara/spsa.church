{
  "name": "spsa.church",
  "version": "1.0.0",
  "description": "St. Peter & St. Andrew Coptic Orthodox Church website",
  "private": true,
  "main": "src/index.tsx",
  "scripts": {
    "test": "TZ=UTC jest",
    "test:cov": "TZ=UTC jest --coverage",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "prettier": "prettier --check 'src/**/*.ts?(x)'",
    "lint": "tslint 'src/**/*.ts?(x)' -c tslint.json",
    "generate:gql": "graphql-codegen --config codegen.yml -r dotenv/config",
    "build": "npm run build:clean && npm run build:js",
    "build:clean": "rm dist/* -rf",
    "build:js": "webpack --config ./webpack.build.ts --env.NODE_ENV=production --env.production --progress",
    "cl": "commitlint --from=HEAD~1",
    "ts:validate": "tsc -p tsconfig.build.json --outDir /tmp",
    "start": "node --max-old-space-size=2048 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack.dev.ts",
    "podman:compose-up": "podman-compose -f docker/docker-compose.yml up",
    "podman:compose-start": "podman-compose -f docker/docker-compose.yml start db cms"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antokara/spsa.church.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "react",
    "redux",
    "graphql",
    "pwa",
    "webapp",
    "website"
  ],
  "author": {
    "name": "Antonios Karagiannis",
    "email": "antokarag@gmail.com",
    "url": "https://antokara.me"
  },
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/antokara/spsa.church/issues"
  },
  "homepage": "https://github.com/antokara/spsa.church#readme",
  "prettier": {
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
